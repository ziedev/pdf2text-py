					"required": true,
					"mimeTypes": {
						"allowed": [
							"application/pdf"
						],
						"default": "application/pdf"
					}
				}
			}
		},
		{
			"outputFolder": {}
		}
	],
	"output": [
		{
			"file": {
				"name": "outfile",
				"type": "text/plain",
				"description": "Text File",
				"options": {
					"filename":"data.txt",
					"mimeTypes": {
						"allowed": [
							"text/plain"
						],
						"default": "text/plain"
					},
					"visualization": false
				}
			}
		}
	],
	"method": {
		"imageType": "docker",
		"imageName": "maaloulmz/pdf2text",
